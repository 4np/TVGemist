{
  "name": "GHKit",
  "version": "0.0.3",
  "summary": "GHKit framework for version checks using GitHub releases",
  "homepage": "https://github.com/4np/GHKit",
  "license": {
    "type": "APACHE",
    "file": "LICENSE"
  },
  "authors": {
    "Jeroen Wesbeek": "github@osx.eu"
  },
  "documentation_url": "https://github.com/4np/GHKit/blob/master/README.md",
  "platforms": {
    "ios": "11.0",
    "osx": "12.0",
    "tvos": "11.0"
  },
  "requires_arc": true,
  "pod_target_xcconfig": {
    "SWIFT_VERSION": "4.0"
  },
  "source": {
    "git": "https://github.com/4np/GHKit.git",
